model TaskDayGraph {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  day    Int
  amount Int
  isFact Boolean

  task   Task   @relation(fields: [taskId], references: [id])
  taskId String @map("task_id")

  @@map("task_day_graph")
}
